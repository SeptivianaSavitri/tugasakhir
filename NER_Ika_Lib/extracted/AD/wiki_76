(d) u + (-u) = 0 ; berarti, u - u = 0
(e) "k" ("m" u) = ("k m") u
(f) "k" (u + v) = "k" u + "k" v
(g) ("k" + "m") u = "k" u + "m" u
(h) 1u = u
